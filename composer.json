{
  "name": "smoren/encryption-tools",
  "description": "Tools for encryption/decryption and signing/verifying (wraps openssl lib)",
  "keywords": ["encryption", "decryption", "rsa", "encrypt", "decrypt", "security", "openssl"],
  "license": "MIT",
  "authors": [
    {
      "name": "Smoren",
      "email": "ofigate@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "smoren/extended-exceptions": "^1.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Smoren\\EncryptionTools\\": "src"
    }
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
